@using MatBlazor;
@inherits Base.Draggable<Weaver.Data.Models.RadioList>;

<MatList>
    @foreach (var btn in Component.Buttons)
    {
        <MatListItem>
            <JRadioButton Component="@btn" ComponentChanged="_ => ComponentChanged.InvokeAsync(Component)" />
        </MatListItem>
    }
</MatList>